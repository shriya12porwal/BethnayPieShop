<!DOCTYPE html>
<html lang="en">
<head>
  <title>Bootstrap Example</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>


<body>
    <header>
        <nav class="navbar navbar-inverse">
          <div class="container-fluid">
            <div class="navbar-header">
              <a class="navbar-brand" href="student/GetStudentCount">.NET BATCH 2</a>
            </div>
            <ul class="nav navbar-nav">
             <li class="nav-item active">
                <a class="nav-link" asp-controller="Student" asp-action="List">Home <span class="sr-only">(current)</span></a>
              </li>
              <li class="dropdown">
                <a class="dropdown-toggle" data-toggle="dropdown" href="#">Team
                <span class="caret"></span></a>
                <ul class="dropdown-menu">
               
              <li class="nav-item">
                <a class="nav-link" asp-controller="Student" asp-action="ListTeamAStudent">TeamA Students</a>
              </li>
                 <li class="nav-item">
                <a class="nav-link" asp-controller="Student" asp-action="ListTeamBStudent">TeamB Students</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" asp-controller="Student" asp-action="ListTeamCStudent">TeamC Students</a>
              </li>
                 <li class="nav-item">
                <a class="nav-link" asp-controller="Student" asp-action="ListTeamDStudent">TeamD Students</a>
              </li>
               <li class="nav-item">
                <a class="nav-link" asp-controller="Student" asp-action="GetStudentsStartingS">S Name Student</a>
              </li>
                </ul>
              </li>
              <li class="nav-item">
                <a class="nav-link" asp-controller="Student" asp-action="ListFeMaleStudent">Female Students</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" asp-controller="Student" asp-action="ListMaleStudent">Male Students</a>
              </li>
            </ul>
          </div>
        </nav>
       
    @RenderBody()
     <main role="main" class="pb-3">
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - BethanyWorkShop - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
